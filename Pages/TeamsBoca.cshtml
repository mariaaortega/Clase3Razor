@page

@{
    var teams= new List <Team>();
        teams.Add(new Team(10, "Boca"));

}
<div class="row">
    <div class="col">
        <table class= "table">
            <head>
                <tr>
                    <th scope= "col"> #Registro</th>
                    <th scope = "col"> Nombre</th>
                </tr>
            </head>
            <body>
                @{
                    foreach (var team in teams)
                    {
                       <tr>
                    <th>#@team.Id </th>
                    <td> @team.Name </td>
                    </tr>
                    }
                }
               
            </body>

        </table>

    </div>
</div>